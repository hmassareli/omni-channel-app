  "name": "@omni/api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "^20.0.0"
  },
  "exports": {
    "./types": "./src/types.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:server": "dotenv -e .env.server -- tsx watch src/index.ts",
    "test": "dotenv -e .env.test -- vitest",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate deploy",
    "db:migrate:server": "dotenv -e .env.server -- prisma migrate deploy",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:generate": "dotenv -e .env -- prisma generate",
    "build": "echo 'Skipping type check - using tsx for runtime'",
    "start": "node dist/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@supabase/supabase-js": "^2.90.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-type-provider-zod": "^6.1.0",
    "pg": "^8.13.1",
    "undici": "^6.19.8",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
